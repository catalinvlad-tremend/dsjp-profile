<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/elements/1.1/">
    <skos:ConceptScheme rdf:about="http://example.com/fruit">
        <skos:hasTopConcept rdf:resource="http://example.com/fruit/citrus-fruit"/>
        <skos:prefLabel xml:lang="en">Fruit</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Frutta</skos:prefLabel>
        <skos:prefLabel xml:lang="pt">Fruta</skos:prefLabel>
        <dc:title xml:lang="en">Fruit</dc:title>
    </skos:ConceptScheme>
    <skos:Concept rdf:about="http://example.com/fruit/citrus-fruit">
        <skos:inScheme rdf:resource="http://example.com/fruit"/>
        <skos:prefLabel xml:lang="en">Citrus fruit</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Agrumi</skos:prefLabel>
        <skos:prefLabel xml:lang="pt">Cítrico</skos:prefLabel>
        <skos:altLabel xml:lang="en">Citrus fruit ALT</skos:altLabel>
        <skos:altLabel xml:lang="fr">Agrumes ALT</skos:altLabel>
        <skos:hiddenLabel xml:lang="en">Citrus fruit HIDDEN</skos:hiddenLabel>
        <skos:example xml:lang="en">lemons, oranges, limes, mandarines, grapefruit, satsumas</skos:example>
        <skos:definition xml:lang="en">Fruit that make you salivate sometimes</skos:definition>
        <skos:dummy xml:lang="en">A dummy value that is not skos.</skos:dummy>
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/exotic-fruit">
        <skos:prefLabel xml:lang="en">Exotic fruit</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://example.com/fruit"/>
        <skos:narrower rdf:resource="http://example.com/fruit/banana" />
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/lemon">
        <skos:inScheme rdf:resource="http://example.com/fruit"/>
        <skos:prefLabel xml:lang="en">Lemon</skos:prefLabel>
        <skos:broader rdf:resource="http://example.com/fruit/citrus-fruit" />
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/pear">
        <skos:inScheme rdf:resource="http://example.com/fruit"/>
        <skos:prefLabel xml:lang="en">Pear</skos:prefLabel>
        <skos:prefLabel xml:lang="fr">Poire</skos:prefLabel>
        <skos:related rdf:resource="http://example.com/fruit/apple"/>
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/apple">
        <skos:inScheme rdf:resource="http://example.com/fruit"/>
        <skos:prefLabel xml:lang="en">Apple</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Mela</skos:prefLabel>
        <skos:prefLabel xml:lang="pt">Maçã</skos:prefLabel>
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/banana">
        <skos:inScheme rdf:resource="http://example.com/fruit"/>
        <skos:prefLabel xml:lang="en">Banana</skos:prefLabel>
    </skos:Concept>
    <skos:Concept rdf:about="http://example.com/fruit/alien">
            <skos:inScheme rdf:resource="http://example.com/fruit"/>
            <skos:prefLabel xml:lang="en">Ålien fruit</skos:prefLabel>
        </skos:Concept>
</rdf:RDF>
